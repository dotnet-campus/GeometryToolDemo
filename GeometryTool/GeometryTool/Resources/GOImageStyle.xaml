<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Border.IsMouseOverBrush" Color="#A6A6A6"/>
    <Style x:Key="GOImageStyle" TargetType="{x:Type Border}">
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="BorderBrush" Value="{StaticResource Border.IsMouseOverBrush}" />
            </Trigger>
       </Style.Triggers>
    </Style>
</ResourceDictionary>